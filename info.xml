<?xml version="1.0"?>
<extension key="com.skvare.journeybuilder" type="module">
  <file>journeybuilder</file>
  <name>Email Marketing Journey Builder</name>
  <description>Advanced email marketing automation with visual journey builder, enhanced Mosaico integration, and powerful personalization features.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sunil Pawar</author>
    <email>sunil@skvare.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Skvare/com.skvare.journeybuilder</url>
    <url desc="Documentation">https://github.com/Skvare/com.skvare.journeybuilder</url>
    <url desc="Support">https://github.com/Skvare/com.skvare.journeybuilder</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-07-27</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.81</ver>
  </compatibility>
  <comments>Journey Builder for CiviCRM - Create automated contact journeys</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Journeybuilder</namespace>
    <format>23.02.1</format>
    <angularModule>crmJourneybuilder</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Journeybuilder_Upgrader</upgrader>
</extension>
